{
  "name": "appfollow-ui",
  "version": "0.2.1",
  "description": "Appfollow UI",
  "scripts": {
    "prepublishOnly": "npm run compile",
    "compile:js": "tsc",
    "compile:css": "postcss src/css/index.css -c .postcssrc.json -o lib/css/index.css",
    "compile": "rm -rf lib && npm run compile:js && npm run compile:css",
    "dev": "styleguidist server",
    "prod": "styleguidist build",
    "css:fix": "stylelint *.css ./src/css --fix; exit 0",
    "js:fix": "eslint --ext .js,.ts,.tsx ./src/app --fix; exit 0"
  },
  "main": "lib/index.js",
  "module": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/AppFollow/appfollow-ui.git"
  },
  "author": "AppFollow",
  "license": "ISC",
  "files": [
    "lib",
    "src"
  ],
  "keywords": [
    "appfollow",
    "app store",
    "google play",
    "ui",
    "library",
    "aso",
    "reviews",
    "keywords"
  ],
  "browserslist": [
    "Chrome >= 70",
    "Safari >= 12",
    "iOS >= 11",
    "Firefox >= 60",
    "Opera >= 43",
    "Edge >= 18",
    "Samsung >= 12"
  ],
  "peerDependencies": {
    "classnames": "^2.3.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@types/classnames": "2.3.1",
    "@types/react": "16.14.5",
    "@typescript-eslint/eslint-plugin": "4.8.2",
    "@typescript-eslint/parser": "4.22.0",
    "autoprefixer": "9.8.6",
    "cache-loader": "4.1.0",
    "classnames": "2.3.1",
    "clean-webpack-plugin": "3.0.0",
    "clipboard-copy": "3.1.0",
    "colors": "1.3.3",
    "cross-env": "5.2.1",
    "css-loader": "3.2.0",
    "eslint": "7.24.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-import-resolver-typescript": "2.3.0",
    "eslint-loader": "3.0.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.21.5",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-plugin-react-perf": "3.3.0",
    "markdown-to-jsx": "6.11.4",
    "mini-css-extract-plugin": "0.5.0",
    "node-sass": "4.12.0",
    "postcss": "7.0.35",
    "postcss-cli": "7.1.2",
    "postcss-csso": "3.0.0",
    "postcss-import": "12.0.1",
    "postcss-loader": "3.0.0",
    "postcss-mixins": "6.2.2",
    "postcss-scss": "2.0.0",
    "precss": "4.0.0",
    "react": "16.13.1",
    "react-docgen-typescript": "1.22.0",
    "react-dom": "16.13.1",
    "react-styleguidist": "11.1.6",
    "stylelint": "9.10.1",
    "stylelint-config-standard": "18.3.0",
    "ts-loader": "8.1.0",
    "typescript": "4.2.4",
    "webpack": "4.39.3",
    "webpack-cli": "3.3.8"
  }
}
